{{#each students}}
<h1 style="text-align: center">{{this.studentUsername}}</h1>

  {{#each this.games }}
  <table width="100%">
  <caption>{{ this.date }}</caption>

    <tr>
      {{#each this.cards}}
      <th>{{ this.letter }}</th>
      {{/each}}
    </tr>
    <tr>
      {{#each this.cards}}
        {{colorCodeCount this.wrong_count}}
      {{/each}}
    </tr>

  </table>
  {{/each}}

{{/each}}
